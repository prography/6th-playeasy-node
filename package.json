    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-kakao": "^1.0.0",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "routing-controllers": "^0.8.1"
  }
